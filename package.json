{
  "name": "memories",
  "version": "1.0.0",
  "description": "The project aims to collect and preserve the memories of residents of cities that suffered from the military aggression of the Russian Federation. It also creates a database of housing destruction to provide an evidence base for the registers of damage caused by this aggression.",
  "main": "./back/server.js",
  "type": "module",
  "scripts": {
    "lint": "eslint . && prettier -w .",
    "test": "node --test .",
    "precommit": "eslint --config .eslintrc.cjs --fix \"front/**/*.{js,cjs,ts,vue}\" \"back/**/*.{js,cjs,ts}\" --ignore-pattern \"front/.nuxt/**\" && prettier --write \"front/**/*.{js,cjs,ts,vue,css}\" \"back/**/*.{js,cjs,ts,css}\"",
    "prepare": "husky install",
    "dev": "cross-env NODE_ENV=development nodemon ./back/server.cjs",
    "start": "cross-env NODE_ENV=production node ./back/server.cjs",
    "db:clean": "cd ./back/sql/&& knex --knexfile ./knexfile.cjs migrate:rollback",
    "db:setup": "cd ./back/sql/ && knex --knexfile ./knexfile.cjs migrate:latest && knex --knexfile ./knexfile.cjs seed:run",
    "devdb:clean": "cd ./back/sql/ && cross-env NODE_ENV=development ./back/sql/knex migrate:rollback",
    "devdb:setup": "cd ./back/sql/ && cross-env NODE_ENV=development ./back/sql/knex migrate:latest && cross-env NODE_ENV=development knex seed:run",
    "knex": "knex --knexfile ./back/sql/knexfile.cjs",
    "dev:f": "concurrently \"npm run dev --prefix ./front\" \"npm run dev\""
  },
  "author": "AlexMel83",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@eslint/js": "^9.9.0",
    "@nuxt/types": "^2.18.1",
    "@nuxt/typescript-build": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "concurrently": "^8.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.0.1",
    "prettier": "^3.3.3",
    "vue-eslint-parser": "^9.4.3"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.24.6",
    "@vee-validate/yup": "^4.13.2",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "eslint": "^8.57.0",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "express-validator": "^7.0.1",
    "geodata": "^0.0.1",
    "ipsp-js-sdk": "^2.0.11-alpha.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "liqpay": "^0.0.1",
    "moment-timezone": "^0.5.45",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-geocoder": "^4.3.0",
    "node-pg-migrate": "^7.5.2",
    "nodemailer": "^6.9.7",
    "openid-client": "^5.6.5",
    "pg": "^8.11.3",
    "set-value": "^4.1.0",
    "update": "^0.4.2",
    "uuid": "^10.0.0"
  }
}
